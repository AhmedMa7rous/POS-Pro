
<cac:InvoiceLine>
  <cbc:ID>#ID#</cbc:ID>
  <cbc:InvoicedQuantity unitCode="PCS">#InvoicedQuantity#</cbc:InvoicedQuantity>
  <cbc:LineExtensionAmount currencyID="SAR">#LineExtensionAmount#</cbc:LineExtensionAmount>
  <cac:TaxTotal>
    <cbc:TaxAmount currencyID="SAR">#TaxAmount#</cbc:TaxAmount>
    <cbc:RoundingAmount currencyID="SAR">#RoundingAmount#</cbc:RoundingAmount>
    <cac:TaxSubtotal>
      <cbc:TaxableAmount currencyID="SAR">#TaxableAmount#</cbc:TaxableAmount>
      <cbc:TaxAmount currencyID="SAR">#TaxAmount#</cbc:TaxAmount>
      <cbc:Percent>#TaxSubtotal_Percent#</cbc:Percent>
      <cac:TaxCategory>
        <cbc:ID>#TaxCategory_ID#</cbc:ID>
        <cbc:Percent>#TaxCategory_Percent#</cbc:Percent>
        <cac:TaxScheme>
          <cbc:ID>#TaxScheme_ID#</cbc:ID>
        </cac:TaxScheme>
      </cac:TaxCategory>
    </cac:TaxSubtotal>
  </cac:TaxTotal>
  <cac:Item>
      #Item_Description#
    <cbc:Name>#Item_NAME#</cbc:Name>
    <cac:ClassifiedTaxCategory>
      <cbc:ID>#ClassifiedTaxCategory_ID#</cbc:ID>
      <cbc:Percent>#ClassifiedTaxCategory_Percent#</cbc:Percent>
      <cac:TaxScheme>
        <cbc:ID>#ClassifiedTaxCategory_TaxScheme_ID#</cbc:ID>
      </cac:TaxScheme>
    </cac:ClassifiedTaxCategory>
  </cac:Item>
  <cac:Price>
    <cbc:PriceAmount currencyID="SAR">#PriceAmount#</cbc:PriceAmount>
    <cbc:BaseQuantity unitCode="PCE">#InvoicedQuantity#</cbc:BaseQuantity>
  </cac:Price>
</cac:InvoiceLine>
